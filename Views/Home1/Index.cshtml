@{
    ViewData["Title"] = "File Upload";
}

<h1>@ViewData["Title"]</h1>

<form method="post" enctype="multipart/form-data" asp-controller="FileUpload" asp-action="UploadFile">
    <input type="file" name="file" />
    <button type="submit">Upload</button>
</form>